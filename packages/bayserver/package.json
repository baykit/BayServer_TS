{
  "name": "bayserver",
  "version": "3.0.0",
  "description": "BayServer is one of the high-speed web servers. It operates as a single-threaded, asynchronous server, which makes it exceptionally fast. It also supports multi-core processors, harnessing the full potential of the CPU's capabilities.\n",
  "main": "bootstrap.js",
  "bin": {
    "bayserver": "bin/bootstrap.js"
  },
  "scripts": {
    "run": "node bin/bootstrap.js -home=. -plan=plan/bayserver.plan",
    "build": "tsc",
    "clean": "rm -f bin/bootstrap.js"
  },
  "keywords": [],
  "author": "Yokohama Baykit",
  "license": "Baykit Public Lisence",
  "homepage": "https://baykit.yokohama/",
  "dependencies": {
    "process": "^0.11.10",
    "typescript": "^4.9.4",
    "bayserver-core": "3.0.0",
    "bayserver-docker-ajp": "3.0.0",
    "bayserver-docker-fcgi": "3.0.0",
    "bayserver-docker-cgi": "3.0.0",
    "bayserver-docker-http": "3.0.0",
    "bayserver-docker-wordpress": "3.0.0",
    "ts-node": "^10.9.1"
  }
}
